<h1>Login</h1>
<div class="row">
      <div class="row">
        <div class="input-field col s12">
           <%= form_for(:session, url: login_login_path) do |f| %>

            <%= f.label :username %>
            <%= f.text_field :username, class: 'form-control' %>
        </div>

        <div class="input-field col s12">
            <%= f.label :password %>
            <%= f.password_field :password, class: 'form-control' %>
        </div>

        <div class="input-field col s12">
            <%= f.submit "Log in", class: "btn btn-primary" %>
        <div>
          <% end %>
      </div>
    </form>
</div> 