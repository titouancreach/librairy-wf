<h1> Show User </h1>


<div class="row">
    <div class"col s12">
      <h5>First name</h5>
    </div>
    <div class="col s12">
      <%= @user.firstname %>
    </div>
</div>

<div class="row">
    <div class"col s12">
      <h5>Last name</h5>
    </div>
    <div class="col s12">
      <%= @user.lastname %>
    </div>
</div>

<div class="row">
    <div class"col s12">
      <h5>Adress 1</h5>
    </div>
    <div class="col s12">
      <%= @user.address1 %>
    </div>
</div>

<div class="row">
    <div class"col s12">
      <h5>Adress 2</h5>
    </div>
    <div class="col s12">
      <%= @user.address2 %>
    </div>
</div>


<div class="row">
    <div class"col s12">
      <h5>Phone</h5>
    </div>
    <div class="col s12">
      <%= @user.phone %>
    </div>
</div>



<div class="row">
    <div class"col s12">
      <h5>Status</h5>
    </div>
    <div class="col s12">
      <%= get_userstatus_from_number(@user.status) %>
    </div>
</div>

         
<%= link_to("/administrator/edituser/" + @user.id.to_s + "/" , class: "waves-effect waves-light btn") do %>
    <i class="material-icons left">edit</i> Edit
<% end %>

